        "/api/createComment": {
            "post": {
                "tags": [
                    "Comments"
                ],
                "summary": "Create a new comment post",
                "description": "Create a new comment post with the provided data",
                "operationId": "93bf8d6cd5bb5e81c021e1b273215a95",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "post_id",
                                    "user_id",
                                    "content"
                                ],
                                "properties": {
                                    "post_id": {
                                        "description": "The ID of the post the comment belongs to",
                                        "type": "integer",
                                        "format": "int64",
                                        "example": "123"
                                    },
                                    "user_id": {
                                        "description": "The ID of the user who posted the comment",
                                        "type": "integer",
                                        "format": "int64",
                                        "example": "456"
                                    },
                                    "content": {
                                        "description": "The content of the comment",
                                        "type": "string",
                                        "example": "This is a great post!"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successfully created a comment post",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "description": "Indicates whether the request was successful",
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "description": "A message describing the outcome of the request",
                                            "type": "string",
                                            "example": "Created comment post successfully!"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request. Invalid input data."
                    },
                    "401": {
                        "description": "Unauthorized. Authentication is required."
                    },
                    "500": {
                        "description": "Internal server error. Failed to create the comment post."
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
